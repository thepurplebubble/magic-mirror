// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data/dev.db"
}

model Message {
  user           String
  originTs       String  @id @unique
  originTeam     String
  originChannel  String
  originThreadTs String?
  mirrorTs       String  @unique
  mirrorTeam     String
  mirrorChannel  String
  mirrorThreadTs String?
  text           String?
  blocks         String?
}
